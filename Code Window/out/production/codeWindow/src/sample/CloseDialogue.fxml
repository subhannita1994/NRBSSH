<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="124.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.InitController">
  <children>
    <AnchorPane maxHeight="233.0" maxWidth="407.0" prefHeight="124.0" prefWidth="407.0" style="-fx-background-color: #33302F;" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="SuccessMsgc" layoutX="9.0" layoutY="17.0" prefHeight="30.0" prefWidth="386.0" text="Session Not Saved, Do you want to save?" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font></Label>
            <Button layoutX="251.0" layoutY="71.0" mnemonicParsing="false"   fx:id="saveyes" onAction="#clickedonSave" text="Yes" />
            <Button layoutX="295.0" layoutY="71.0" mnemonicParsing="false" fx:id="saveno" onAction="#clickedonNoSave" text="No" />
            
         </children>
    </AnchorPane>
  </children>
</VBox>
